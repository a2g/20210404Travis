{
	"$id": "https://example.com/arrays.schema.json",
	"$schema": "http://json-schema.org/draft-07/schema#",
	"description": "A representation of a person, company, organization, or place",
	"type": "object",
	"properties": {
		"objects": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"transactions": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/transaction_type"
			}
		}
	},
	"definitions": {
		"prop_type": {
			"enum": ["map",	"compass"]
		},
		"transaction_type": {
			"type": "object",
			"required": [
				"inputA",
				"inputB"
			],
			"anyOf": [
				{
					"description": "A transforms B into C",
					"properties": {
						"inputA": {
							"$ref": "#/definitions/prop_type"
						},
						"inputB": {
							"$ref": "#/definitions/prop_type"
						}
					}
				},
				{
					"description": "A and B generates C",
					"properties": {
						"inputA": {
							"$ref": "#/definitions/prop_type"
						},
						"inputB": {
							"$ref": "#/definitions/prop_type"
						}
					}
				},
				{
					"description": "Creates C",
					"properties": {
						"inputA": {
							"$ref": "#/definitions/prop_type"
						},
						"inputB": {
							"$ref": "#/definitions/prop_type"
						}
					}
				}
			]
		}
	}
}